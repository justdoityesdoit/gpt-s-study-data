console.log("Hello World!");



var x = 10;
let y = 20;
const z = 30;



if (x > y) {
  console.log("x is greater than y");
} else {
  console.log("y is greater than x");
}



for (let i = 0; i < 5; i++) {
  console.log(i);
}

let j = 0;
while (j < 5) {
  console.log(j);
  j++;
}



function addNumbers(x, y) {
  return x + y;
}

let result = addNumbers(5, 10);
console.log(result);



let fruits = ["apple", "banana", "orange"];

console.log(fruits[0]); // Output: apple

fruits.push("pear"); // Add a new element to the end of the array

console.log(fruits.length); // Output: 4



let person = {
  name: "John",
  age: 30,
  city: "New York"
};

console.log(person.name); // Output: John

person.age = 40; // Update the age property

console.log(person); // Output: {name: "John", age: 40, city: "New York"}



